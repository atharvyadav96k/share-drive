  <style>
    .custom-box {
      max-width: 500px;
      border: 1px solid #ddd;
      border-radius: 10px;
      box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);
      overflow: hidden;
    }

    .custom-header {
      background-color: #007bff;
      color: white;
      font-weight: bold;
      padding: 10px 15px;
    }

    .custom-content {
      padding: 15px;
    }

    .custom-row {
      display: flex;
      justify-content: space-between;
      align-items: center;
      margin-top: 10px;
    }

    .form-control,
    .form-select {
      width: 100%;
    }

    @media screen and (max-width: 450px) {
      .carousel-image {
        object-fit: contain;
        /* Ensures the image covers the area without stretching */
        max-height: 450px;
        /* Set to match the max-height of the carousel */
      }
    }

    .carousel-image {
      object-fit: contain;
      max-height: 450px;
      object-position: center;
    }

    .horizontal {
      display: flex;
      overflow-x: auto;
      padding-bottom: 10px;
    }

    .horizontal::-webkit-scrollbar {
      width: 0px;
    }
  </style>
</head>

